{
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "myc": ["src"],
      "myc/*": ["src/*", "*"],
      "utils/*": ["utils/*"]
    }
  },
  "include": [".dumirc.ts", "src/**/*", "utils/**/*"]
}
